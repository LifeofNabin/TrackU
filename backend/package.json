{
  "name": "tracku-backend",
  "version": "1.0.0",
  "description": "TrackU Study Guardian - Real-time study monitoring backend",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:seed": "npx sequelize-cli db:seed:all",
    "db:reset": "npx sequelize-cli db:migrate:undo:all && npm run db:migrate && npm run db:seed",
    "build": "echo 'No build step required'",
    "postinstall": "echo 'Backend dependencies installed successfully'",
    "docker:build": "docker build -t tracku-backend .",
    "docker:run": "docker run -p 8000:8000 tracku-backend"
  },
  "keywords": [
    "study-tracking",
    "face-detection",
    "real-time-monitoring",
    "education",
    "productivity"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "express-validator": "^7.0.1",
    "express-rate-limit": "^6.8.1",
    "sequelize": "^6.32.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "redis": "^4.6.7",
    "winston": "^3.10.0",
    "multer": "^1.4.5-lts.1",
    "uuid": "^9.0.0",
    "moment": "^2.29.4",
    "lodash": "^4.17.21",
    "compression": "^1.7.4",
    "@socket.io/redis-adapter": "^8.2.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.2",
    "supertest": "^6.3.3",
    "eslint": "^8.45.0",
    "eslint-config-node": "^4.1.0",
    "eslint-plugin-node": "^11.1.0",
    "sequelize-cli": "^6.6.1",
    "@types/jest": "^29.5.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/tracku-backend.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/tracku-backend/issues"
  },
  "homepage": "https://github.com/yourusername/tracku-backend#readme"
}